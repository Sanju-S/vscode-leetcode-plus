{
  "name": "vscode-leetcode-plus",
  "displayName": "vscode-leetcode-plus",
  "description": "Solve and submit LeetCode problems directly from VS Code â€” with language and difficulty filters, WebView, and live result panel.",
  "version": "1.0.0",
  "publisher": "sanju-sarkar",
  "license": "MIT",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.105.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sanju-S/vscode-leetcode-plus.git"
  },
  "homepage": "https://github.com/Sanju-S/vscode-leetcode-plus#readme",
  "categories": [
    "Programming Languages", "Education"
  ],
  "activationEvents": [
    "onCommand:vscode-leetcode-plus.helloWorld",
    "onCommand:vscode-leetcode-plus.setSession",
    "onCommand:vscode-leetcode-plus.getRandomProblem",
    "onCommand:vscode-leetcode-plus.submitSolution"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-leetcode-plus.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "vscode-leetcode-plus.setSession",
        "title": "LeetCode Plus: Set Session Cookie"
      },
      {
        "command": "vscode-leetcode-plus.getRandomProblem",
        "title": "LeetCode Plus: Get Random Problem"
      },
      {
        "command": "vscode-leetcode-plus.submitSolution",
        "title": "LeetCode Plus: Submit Solution"
      },
      {
        "command": "vscode-leetcode-plus.changeLanguage",
        "title": "LeetCode Plus: Change Preferred Language"
      },
      {
        "command": "vscode-leetcode-plus.changeDifficulty",
        "title": "LeetCode Plus: Change Preferred Difficulty"
      }
    ],
    "keybindings": [
      {
        "command": "vscode-leetcode-plus.getRandomProblem",
        "key": "ctrl+alt+l",
        "mac": "cmd+alt+l",
        "when": "editorTextFocus"
      },
      {
        "command": "vscode-leetcode-plus.submitSolution",
        "key": "ctrl+alt+s",
        "mac": "cmd+alt+s",
        "when": "editorTextFocus"
      },
      {
        "command": "vscode-leetcode-plus.setSession",
        "key": "ctrl+alt+c",
        "mac": "cmd+alt+c"
      },
      {
        "command": "vscode-leetcode-plus.changeLanguage",
        "key": "ctrl+alt+p",
        "mac": "cmd+alt+p"
      }
    ],
      "configuration": {
      "title": "LeetCode Plus",
      "properties": {
        "leetcodePlus.autoSaveOnFetch": {
          "type": "boolean",
          "default": true,
          "description": "Automatically save fetched problems to disk."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/node-fetch": "^2.6.13",
    "@types/vscode": "^1.105.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.36.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "webpack": "^5.102.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "node-fetch": "^2.7.0"
  }
}
